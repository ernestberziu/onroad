<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.onroad">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_directions_bus_24"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".editLines.editLines"></activity>
        <activity android:name=".viewLinesAgency.viewaAgencyLines" />
        <activity android:name=".addLines.addFrom.addFrom" />
        <activity android:name=".addLines.addTo.addTo" />
        <activity android:name=".addLines.addLines" />
        <activity android:name=".launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".loginandregister.login" />
        <activity android:name=".loginandregister.Register" />
        <activity android:name=".Routes.Routes" />
        <activity android:name=".returnLines.Return" />
        <activity android:name=".toSearch.toSearch" />
        <activity android:name=".fromSearch.fromSearch" />
        <activity android:name=".departLines.discover" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>